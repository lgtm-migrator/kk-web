{
  "name": "kk-web",
  "description": "Freelance front-end programmer piro's website",
  "version": "0.1.0",
  "author": "piro <piro.haniwa@gmail.com>",
  "homepage": "https://kk-web.link",
  "dependencies": {
    "@types/fs-extra": "9.0.1",
    "@types/no-scroll": "2.1.0",
    "@types/pdfmake": "0.1.14",
    "@types/react-css-modules": "4.6.2",
    "@types/react-helmet": "6.0.0",
    "@types/react-no-ssr": "1.1.1",
    "@types/uniqid": "5.2.0",
    "@types/yup": "0.29.3",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "dayjs": "1.8.29",
    "eslint": "7.5.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "fs-extra": "9.0.1",
    "gatsby": "2.22.15",
    "gatsby-image": "2.4.5",
    "gatsby-plugin-google-adsense": "1.1.3",
    "gatsby-plugin-google-analytics": "2.3.6",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-google-tagmanager": "2.3.7",
    "gatsby-plugin-manifest": "2.4.9",
    "gatsby-plugin-mixpanel": "3.1.6",
    "gatsby-plugin-offline": "3.2.7",
    "gatsby-plugin-react-css-modules": "2.3.4",
    "gatsby-plugin-react-helmet": "3.3.2",
    "gatsby-plugin-remove-trailing-slashes": "2.3.7",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sass": "2.3.4",
    "gatsby-plugin-sharp": "2.6.9",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "gatsby-source-filesystem": "2.3.12",
    "gatsby-transformer-remark": "2.8.17",
    "gatsby-transformer-sharp": "2.5.3",
    "github-markdown-css": "4.0.0",
    "logrocket": "1.0.10",
    "logrocket-react": "4.0.1",
    "mixpanel-browser": "2.38.0",
    "no-scroll": "2.1.1",
    "node-sass": "4.14.1",
    "pdfmake": "0.1.66",
    "postcss-scss": "2.1.1",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-bottom-scroll-listener": "4.1.0",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "6.0.8",
    "react-icons-kit": "1.3.1",
    "react-no-ssr": "1.1.0",
    "react-toastify": "6.0.8",
    "ress": "2.0.4",
    "sass-mq": "5.0.1",
    "ts-node": "8.10.2",
    "uniqid": "5.2.0",
    "use-dark-mode": "2.3.1",
    "yup": "0.29.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run build:fonts && gatsby build",
    "build:fonts": "ts-node --project pdfmake/tsconfig.json pdfmake/buildFonts.ts",
    "deploy": "firebase deploy --only hosting",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --fix --ext .ts,.tsx src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piro0919/kk-web2"
  },
  "bugs": {
    "url": "https://github.com/piro0919/kk-web2/issues"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "plugin:jsx-a11y/strict",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": "src"
        }
      }
    },
    "rules": {
      "no-alert": "error",
      "no-console": "error",
      "no-var": "error",
      "import/order": [
        "error",
        {
          "newlines-between": "always"
        }
      ],
      "react/jsx-fragments": [
        "error",
        "syntax"
      ],
      "react/jsx-sort-props": [
        "error"
      ],
      "sort-keys": "error"
    }
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  }
}
