  "description": "Freelance front-end programmer piro's website",
  "eslintConfig": {
    "extends": [
      "airbnb-typescript",
      "airbnb/hooks",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "overrides": [
      {
        "files": [
          "**/*.tsx"
        ],
        "rules": {
          "react/prop-types": "off"
        }
      }
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/indent": "off",
      "import/order": [
        "error",
        {
          "alphabetize": {
            "caseInsensitive": true,
            "order": "asc"
          },
          "newlines-between": "never"
        }
      ],
      "no-alert": "error",
      "no-console": "error",
      "no-var": "error",
      "react/jsx-indent": "off",
      "react/jsx-sort-props": [
        "error"
      ],
      "sort-keys": "error"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": "src"
        }
      }
    }
  },
  "eslintIgnore": [
    "pdfmake"
  ],
  "homepage": "https://kk-web.link",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.scss": "npm run lint:css",
    "**/*.ts?(x)": "npm run lint"
  },
  "name": "kk-web",
  "repository": {
    "type": "git",
    "url": "https://github.com/piro0919/kk-web"
  },
  "scripts": {
    "build": "npm run build:fonts && gatsby build",
    "build:fonts": "ts-node --project pdfmake/tsconfig.json pdfmake/buildFonts.ts",
    "clean": "gatsby clean",
    "deploy": "firebase deploy --only hosting",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{md}\"",
    "lint": "eslint --fix --ext .ts,.tsx src/",
    "lint:css": "stylelint \"**/*.scss\" --fix",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-sass-guidelines"
    ],
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "color-named": [
        "never",
        {
          "ignore": [
            "inside-function"
          ]
        }
      ],
      "order/properties-alphabetical-order": true,
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "version": "0.1.0"
}
