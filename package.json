  "description": "Freelance front-end programmer piro's website",
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:jsx-a11y/strict",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "alphabetize": {
            "caseInsensitive": true,
            "order": "asc"
          },
          "newlines-between": "always"
        }
      ],
      "no-alert": "error",
      "no-console": "error",
      "no-var": "error",
      "react/jsx-fragments": [
        "error",
        "syntax"
      ],
      "react/jsx-sort-props": [
        "error"
      ],
      "sort-keys": "error"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": "src"
        }
      }
    }
  },
  "homepage": "https://kk-web.link",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run precommit"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.scss": "npm run stylelint",
    "**/*.ts?(x)": "npm run lint"
  },
  "name": "kk-web",
  "repository": {
    "type": "git",
    "url": "https://github.com/piro0919/kk-web"
  },
  "scripts": {
    "build": "npm run build:fonts && gatsby build",
    "build:fonts": "ts-node --project pdfmake/tsconfig.json pdfmake/buildFonts.ts",
    "clean": "gatsby clean",
    "deploy": "firebase deploy --only hosting",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{ts,tsx,scss,md}\"",
    "format:staged": "pretty-quick --staged --pattern \"**/*.*(ts|tsx|scss|md)\"",
    "lint": "eslint --fix --ext .ts,.tsx src/",
    "lint:staged": "lint-staged",
    "precommit": "npm run format:staged && npm run lint:staged",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-sass-guidelines",
      "stylelint-prettier/recommended"
    ],
    "plugins": [
      "stylelint-prettier"
    ],
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-after-same-name-blockless"
          ]
        }
      ],
      "color-named": [
        "never",
        {
          "ignore": [
            "inside-function"
          ]
        }
      ],
      "prettier/prettier": true
    }
  },
  "version": "0.1.0"
}
